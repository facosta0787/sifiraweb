{
  "name": "sfr-ui",
  "version": "1.0.0",
  "description": "Front-End for sifira web project",
  "private": true,
  "scripts": {
    "start": "concurrently -n \"frontend,api\" \"npm run start:client\" \"cd ../sfr-api && npm run dev\"",
    "start:client": "REACT_APP_API=localhost:3000 PORT=5000 node scripts/start.js",
    "build": "REACT_APP_API=localhost:3000 node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "serve": "serve -s build",
    "docker:start": "npm run docker:build && npm run docker:run",
    "docker:build": "docker build -t sifiraweb-ui .",
    "docker:run": "docker run -d --rm --name sifira-ui -p 80:3000 sifiraweb-ui",
    "docker:clear": "docker stop sifira-ui && docker rmi sifiraweb-ui && docker images"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facosta0787/sifiraweb"
  },
  "keywords": [
    "sifira",
    "sifiraweb",
    "react",
    "javascript"
  ],
  "author": "@facosta0787",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.2",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^4.1.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-range": "2.1.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.5.1",
    "react-datepicker": "^1.6.0",
    "react-dev-utils": "^3.1.0",
    "react-dom": "^16.5.1",
    "react-error-overlay": "^1.0.10",
    "react-input-calendar": "^0.5.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.5",
    "style-loader": "0.18.2",
    "styled-components": "^2.1.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "serve": "^10.1.1"
  }
}
